{
  "packageRules": [
    {
        "description": ["Auto-merge container digests updates"],
        "matchDatasources": ["docker"],
        "automerge": true,
        "automergeType": "pr",
        "matchUpdateTypes": ["digest"]
    },
    {
        "description": ["Auto-merge patch updates"],
        "automerge": true,
        "automergeType": "pr",
        "matchUpdateTypes": ["patch"]
    }
  ]
}
